<% if operatingsystem != 'windows' %>
PidFile=/var/run/zabbix/zabbix_agentd.pid
LogFile=/var/log/zabbix/zabbix_agentd.log
Include=/etc/zabbix/zabbix_agentd.d/
Include=/etc/zabbix/zabbix_custom.d/
<% else %>
LogFile=c:\zabbix\zabbix_agentd.log
#Include=c:\zabbix\zabbix_agentd.d\
#Include=c:\zabbix\zabbix_custom.d\
<% end%>

LogFileSize=0
Server=127.0.0.1<% @zbx_Servers.each do |zbx_Server| %>,<%= zbx_Server %><% end %>
ServerActive=127.0.0.1<% @zbx_Servers.each do |zbx_Server| %>,<%= zbx_Server %><% end %>
Hostname=<%= @hostname %>
AllowRoot=1



